{
  "name": "@itneris/pickers",
  "version": "1.0.5",
  "description": "Custom ItNeris organization datepicker based on MaterialUI",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "date-fns": "^2.29.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.10.4",
    "@emotion/styled": ">=11.10.4",
    "@mui/icons-material": ">=5.10.3",
    "@mui/material": ">=5.10.3",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.css dist/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "yarn clean && tsc && yarn copy-files",
    "tsc": "tsc",
    "publish:npm": "(if exist dist rmdir /s /q dist) && mkdir dist && babel ./src/components -d dist --copy-files"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itneris/pickers.git"
  },
  "author": "itneris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itneris/pickers/issues"
  },
  "homepage": ".https://github.com/itneris/pickers#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
